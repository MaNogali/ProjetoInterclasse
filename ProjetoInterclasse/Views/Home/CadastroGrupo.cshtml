@model ProjetoInterclasse.Models.Grupo

<h2>Cadastro de Grupo</h2>

@using (Html.BeginForm())
{
    <div>
        @Html.LabelFor(m => m.IdGrupo)
        @Html.TextBoxFor(m => m.IdGrupo)
    </div>
    <div>
        @Html.LabelFor(m => m.IdModalidade)
        @Html.TextBoxFor(m => m.IdModalidade)
    </div>
    <div>
        @Html.LabelFor(m => m.Time1)
        @Html.TextBoxFor(m => m.Time1)
    </div>
    <div>
        @Html.LabelFor(m => m.Time2)
        @Html.TextBoxFor(m => m.Time2)
    </div>
    <div>
        @Html.LabelFor(m => m.Time3)
        @Html.TextBoxFor(m => m.Time3)
    </div>
    <div>
        @Html.LabelFor(m => m.Time4)
        @Html.TextBoxFor(m => m.Time4)
    </div>
    <button type="submit">Cadastrar</button>
}
